function s() {
	if [ "$1" == "" ]; then
		echo "s kr칝ver s칮gefrase, f.eks. 's 2022-01 l칮n'"
		return
	fi
	echo "searching for $@  in $LEDGER_BEGIN - $LEDGER_END - use ss for searching in all time"
	php /svn/svnroot/Applications/key.php search $LEDGER_BEGIN $LEDGER_END "$@"
}
